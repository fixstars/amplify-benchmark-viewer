{
  "name": "amplify_benchmark_viewer",
  "homepage": "https://fixstars.github.io/amplify-benchmark-viewer",
  "version": "0.0.0",
  "dependencies": {
    "@mui/x-data-grid": "^6.11.1",
    "@tanstack/react-query": "^4.32.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.4.0",
    "d3": "^7.8.5",
    "json-schema-to-typescript": "^13.0.2",
    "plotly.js": "^2.25.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "typescript": "5.0.4",
    "web-vitals": "^3.4.0"
  },
  "scripts": {
    "deploy": "gh-pages -d public",
    "start": "react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.(css|tsx)'",
    "lint:md": "remark . --quiet --frail",
    "check:spell": "cspell '**' --gitignore --no-progress",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.13.5",
    "@storybook/addon-actions": "^7.3.1",
    "@storybook/addon-essentials": "^7.3.0",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/node-logger": "^7.3.0",
    "@storybook/preset-create-react-app": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.3.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/d3": "^7.4.0",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "axios-mock-adapter": "^1.21.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "cspell": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^5.0.0",
    "history": "^5.3.0",
    "lefthook": "^1.4.9",
    "postcss-styled-syntax": "^0.4.0",
    "prop-types": "^15.8.1",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "storybook": "^7.2.3",
    "storybook-addon-react-router-v6": "^2.0.4",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "webpack": "^5.88.2"
  },
  "overrides": {
    "postcss": "^8.4.20"
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!axios|d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  }
}
