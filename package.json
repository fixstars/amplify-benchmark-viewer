{
  "name": "template",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/x-data-grid": "^5.17.13",
    "@tanstack/react-query": "^4.29.15",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "axios": "^1.4.0",
    "d3": "^7.8.5",
    "json-schema-to-typescript": "^13.0.2",
    "plotly.js": "^2.24.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.13.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "typescript": "^5.1.3",
    "web-vitals": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.(css|tsx)'",
    "lint:md": "remark . --quiet --frail",
    "check:spell": "cspell '**' --gitignore --no-progress",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.10.15",
    "@storybook/addon-actions": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-interactions": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/builder-webpack5": "^7.0.22",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.0.23",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.23",
    "@storybook/testing-library": "^0.2.0",
    "@types/d3": "^7.4.0",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.60.0",
    "axios-mock-adapter": "^1.21.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "cspell": "^6.31.1",
    "eslint": "^8.43.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "history": "^5.3.0",
    "lefthook": "^1.4.3",
    "postcss-styled-syntax": "^0.4.0",
    "prop-types": "^15.8.1",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "sb": "^6.5.13",
    "storybook-addon-react-router-v6": "^1.0.2",
    "stylelint": "^15.8.0",
    "stylelint-config-standard": "^32.0.0",
    "webpack": "^5.88.0"
  },
  "overrides": {
    "postcss": "^8.4.20"
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!axios|d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  }
}
